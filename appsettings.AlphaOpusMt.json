{
  "AlphaOpusMt": {
    "IsEnabled": true,
    "ModelsDirectory": "Models/OpusMT",
    "MaxSequenceLength": 256,
    "MemoryLimitMb": 300,
    "ThreadCount": 2,
    "SupportedLanguagePairs": [
      {
        "SourceLanguage": "ja",
        "TargetLanguage": "en",
        "ModelName": "opus-mt-ja-en",
        "Description": "Japanese to English translation"
      },
      {
        "SourceLanguage": "en",
        "TargetLanguage": "ja",
        "ModelName": "opus-mt-en-ja",
        "Description": "English to Japanese translation"
      }
    ]
  },
  "Translation": {
    "AlphaTest": {
      "EnableLocalTranslation": true,
      "DefaultEngine": "OpusMTAlpha",
      "FallbackToCloud": false,
      "MaxConcurrentTranslations": 2
    }
  },
  "Logging": {
    "LogLevel": {
      "Baketa.Infrastructure.Translation.Local.Onnx": "Information"
    }
  }
}